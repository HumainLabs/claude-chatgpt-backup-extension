{
  "manifest_version": 2,
  "name": "HumainLabs Claude Chat Backup",
  "version": "1.0",
  "description": "Backs up Claude.ai conversations. Created by HumainLabs.ai.",
  "permissions": [
    "cookies",
    "downloads",
    "notifications",
    "tabs",
    "https://claude.ai/*"
  ],
  "background": {
    "scripts": ["chat_export_background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "HumainLabs Claude Backup",
    "default_popup": "html/chat_export_popup.html",
    "default_icon": {
      "48": "icons/icon48.png",
      "96": "icons/icon96.png"
    }
  },
  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "claude-chat-backup@humainlabs.ai",
      "strict_min_version": "57.0"
    }
  }
}
